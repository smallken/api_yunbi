<table class="table">
<tr>
  <th>Ticker ID</th>
  <th>Ticker Name</th>
  <th>时间戳</th>
  <th>购买报价</th>
  <th>出售报价</th>
  <th>最低价</th>
  <th>最高价</th>
  <th>最后成交价</th>
  <th>交易总量</th>
  <th>价格</th>
</tr>
<% @tickers.each do |ticker| %>
  <tr>
    <td><%= ticker.id %></td>
    <td><%= ticker.ticker_name %></td>
    <td><%= ticker.timestamp_at %></td>
    <td><%= ticker.price_buy %></td>
    <td><%= ticker.price_sell %></td>
    <td><%= ticker.price_low %></td>
    <td><%= ticker.price_high %></td>
    <td><%= ticker.price_last %></td>
    <td><%= ticker.trade_vol %></td>
    <td><%= ticker.current_price %></td>
    <td>
      <%= link_to "数据更新", update_price_ticker_path(ticker), :method => :post %>
    </td>
  </tr>
<% end %>
</table>
